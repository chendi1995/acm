#include<iostream>
#include<stdio.h>
#include<stdlib.h>
#include<string.h>
#include<algorithm>
using namespace std;

struct Matrix
{
	int a[2][2];
	memset(a,0,sizeof(a));
};
typedef struct Matrix M;

int main()
{
	int p,q,n;
	int B[2][1];
	int t;
	while(scanf("%d%d%d",&p,&q,&n)&&!(p==0&&q==0))
	{
		M A;
		A.a[0][0]=p;
		A.a[0][1]=-q;
		A.a[1][0]=1;
		A.a[1][1]=0;
		B.a[0][0]=p;
		B.a[1][0]=2;
		if(n==0)
		{
			printf("%d\n",B.a[1][0]);
		}
		else if(n==1)
			printf("%d\n",p);
		else 
		{
			t=n-1;
			while(t>0)

		}

	}
}
