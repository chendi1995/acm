#include<iostream>
#include<stdio.h>
#include<stdlib.h>
#include<string.h>
#include<algorithm>
#define MAXN 100010
using namespace std;
int add[MAXN<<2];
typedef struct
{
	int value;
	int left;
	int right;
}NODE;
NODE node[MAXN<<2];

void build(int rt,int left,int right)
{
	int mid;
	node[rt].left=left;
	node[rt].right=right;
	if(left==right)
	{
		node[rt].v=1;
		return;
	}
	mid=(left+right)>>1;
	build(rt<<1,left,mid);
	build(rt<<1|1,mid+1,right);
}

int main()
{
	int T;
	int N;
	int Q;
	int i;
	int m,n,v;
	scanf("%d",&T);
	while(T--)
	{
		scanf("%d",&N);
		build(1,1,N);
		scanf("%d",&Q);
		for(i=1;i<=Q;i++)
		{
			scanf("%d%d%d",&m,&n,&v);
			update(m,n,v);
		}
	}
}
